    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "dpc-sdp/tide_core": "^1.6.11",
        "dpc-sdp/tide_event": "^1.3.1",
        "dpc-sdp/tide_media": "^1.5.1",
        "dpc-sdp/tide_automated_listing": "dev-feature/SDPA-4541-updates"
